alter procedure prc$dat_decode_date (ADATE date)
returns (YEAR integer, MONTH integer,
DAY integer)
as
BEGIN
  EXECUTE PROCEDURE PRC$DAT_GET_YEAR_NUMBER(ADate) RETURNING_VALUES 
Year;
  EXECUTE PROCEDURE PRC$DAT_GET_MONTH_NUMBER(ADate) RETURNING_VALUES 
Month;
  EXECUTE PROCEDURE PRC$DAT_GET_DAY_OF_MONTH(ADate) RETURNING_VALUES 
Day;
END
