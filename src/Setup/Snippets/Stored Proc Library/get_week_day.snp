alter procedure prc$dat_get_week_day (ADATE date)
returns (RESULT integer)
as
DECLARE VARIABLE Elapsed INTEGER;
DECLARE VARIABLE Temp  INTEGER;
BEGIN
  EXECUTE PROCEDURE PRC$DAT_DIF_IN_DAYS("1.1.96", ADate) 
RETURNING_VALUES Elapsed;
  EXECUTE PROCEDURE PRC$DBL_GEN_MODULAS(Elapsed, 7) RETURNING_VALUES 
temp;
  RESULT = CAST(temp as INTEGER);
END
