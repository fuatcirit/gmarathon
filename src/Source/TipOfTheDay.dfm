    ItemHeight = 13
    Items.Strings = (
      
        '^Bill Karwin'#39's Mantra: %s   NULL is a state, not a value. %s   N' +
        'ULL is a state, not a value. %s   NULL is a state, not a value.'
      
        '?You can print a report on a metadata object by selecting it in ' +
        'the Database Manager and selecting Print from the shortcut menu.'
      
        '?You can drag and drop stored procedures and their arguments fro' +
        'm the SQL Assistant into any SQL Editor'
      
        '?You can export the contents of a dataset grid by choosing Save ' +
        'To File from the file menu'
      
        '?You can save often used pieces of code to the SQL Code Snippets' +
        ' catalog by selecting the text and choosing Capture Code Snippet' +
        ' from the Edit menu'
      
        '?You can drag and drop relation columns from the SQL Assistant i' +
        'nto any SQL Editor'
      '?You can drag code snippets from the SQL Code Snippets Catalog'
      
        '?You can create a database by choosing Create Database from the ' +
        'File menu'
      
        '?You can view Query Plans by checking View Query Plan on the Obj' +
        'ect menu in the SQL Statement Editor'
      
        '?You can record your changes to the current database as you work' +
        ' by choosing Record New Script from the script menu'
      
        '?You can search metadata for a specific string by choosing Searc' +
        'h Metadata from the tools menu'
      
        '?You can get help on SQL Syntax by selecting a keyword in any SQ' +
        'L Editor and pressing F1'
      
        '?You can drag Syntax diagrams from the SQL Syntax Catalog by cho' +
        'osing SQL Syntax from the Tools menu'
      
        '?You can access code templates by pressing CTRL-J. To add to or ' +
        'change templates choose Options from the Tools menu')
    TabOrder = 5
    Visible = False
  end
